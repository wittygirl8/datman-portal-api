{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "account enroll Request",
    "required": [
        "firstname",
        "lastname",
        "email",
        "client_ref_id",
        "account_type",
        "phone"
    ],
    "properties": {
        "firstname": {
            "type": "string",
            "title": "Frist name of the user",
            "pattern": "^(.*)$",
            "example": "Fedrick"
        },
        "lastname": {
            "type": "string",
            "title": "Last name of the user",
            "pattern": "^(.*)$",
            "example": "Rich"
        },
        "email": {
            "type": "string",
            "title": "Email of the user",
            "pattern": "^(.*)$",
            "example": "Fedrick@xyz.com"
        },
        "phone": {
            "type": "string",
            "title": "phone no. of the user",
            "pattern": "^(.*)$",
            "example": "07123456789"
        },
        "client_ref_id": {
            "type": "string",
            "title": "Unique refrence associated for user",
            "pattern": "^(.*)$",
            "example": "I_AM_VERY_UNIQUE_REF_PER_USER"
        },
        "account_type": {
            "type": "string",
            "title": "Account type of user, END_USER/INDIVIDUAL",
            "pattern": "^(.*)$",
            "example": "END_USER"
        }
    }
}